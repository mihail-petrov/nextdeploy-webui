<article class="note note-partial">
            <h2 class="note-title">{{{title}}}</h2>
            <div class="note-excerpt">
                {{#if isTwitter}}
                  {{#twitter-feed}}
                    <p>{{yield}}</p>
                  {{/twitter-feed}}
                {{else}}
                  <p>{{yield}}</p>
                {{/if}}
            </div>
</article><br/>